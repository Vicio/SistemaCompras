<UserControl x:Class="ComprasUAQ.Windows.Elements.Pendientes"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ComprasUAQ.Windows.Elements"
             mc:Ignorable="d" 
             d:DesignHeight="220" d:DesignWidth="180">
    <Grid HorizontalAlignment="Left" Height="220" Margin="0,0,0,0" VerticalAlignment="Top" Width="180" Background="#FFFFFF">
        <Border Background="#D9D9D9" BorderThickness="1,1,1,1" CornerRadius="9,9,9,9">
        </Border>
        <Grid HorizontalAlignment="Left" Height="25" Margin="0,0,0,0" VerticalAlignment="Top" Width="180" Background="#FFFFFF">
            <Border Background="#121548" BorderThickness="1,1,1,1" CornerRadius="9,9,0,0">
            </Border>
            <Label x:Name="label1"  Content="Pendientes" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Top" FontSize="12" Foreground="#FFFFFF"/>
        </Grid>
        <ListBox x:Name="listBoxPendientes" HorizontalAlignment="Left" Height="195" Margin="0,25,0,0" VerticalAlignment="Top" Width="180" Background="#D9D9D9" FontFamily="FuturaStd">
        </ListBox>


    </Grid>
</UserControl>
